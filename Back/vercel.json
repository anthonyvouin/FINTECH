{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/contact",
            "dest": "/api/contact"
        },
        {
            "src": "/api/reponse",
            "dest": "/api/reponse"
        },
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ]
}